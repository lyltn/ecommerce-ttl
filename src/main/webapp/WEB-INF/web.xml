<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>ecommerce ttl</display-name>
	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>
	<jsp-config>
		<taglib>
			<taglib-uri>http://java.sun.com/jsp/jstl/core</taglib-uri>
			<taglib-location>/WEB-INF/lib/c.tld</taglib-location>
		</taglib>
		<!-- Other taglib configurations -->

	</jsp-config>
	
	<servlet>
    <servlet-name>CliAddImgPro</servlet-name>
    <servlet-class>controller.client.CliAddImgPro</servlet-class>
    <multipart-config>
        <max-file-size>16177215</max-file-size>
        <max-request-size>2147483648</max-request-size>
        <file-size-threshold>0</file-size-threshold>
    </multipart-config>
   </servlet>
	
	
</web-app>